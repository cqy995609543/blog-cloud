<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.chenshu.mapper.MenuMapper">


    <select id="getMenuList" resultType="team.chenshu.entity.Menu">

        select menu_id,menu_name from blogs_menu where menu_target != 1 ;

    </select>

    <select id="getMenuType" resultType="team.chenshu.entity.Menu">

        select menu_id,menu_name from blogs_menu where menu_target = 2 ;

    </select>





</mapper>
